{
  "name": "agent",
  "version": "1.0.0",
  "description": "CLI tool built with TypeScript",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "ci": "pnpm run typecheck && pnpm run lint && pnpm run format:check"
  },
  "keywords": [
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2.0.0-beta.12",
    "ai": "5.0.0-beta.33",
    "dotenv": "^17.2.1"
  }
}
